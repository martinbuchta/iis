<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block title %}Vstupenky{% endblock %}</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="/iis/public/AdminLTE-3.0.1/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Tempusdominus Bbootstrap 4 -->
    <link rel="stylesheet" href="/iis/public/AdminLTE-3.0.1/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="/iis/public/AdminLTE-3.0.1/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- JQVMap -->
    <link rel="stylesheet" href="/iis/public/AdminLTE-3.0.1/plugins/jqvmap/jqvmap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="/iis/public/AdminLTE-3.0.1/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="/iis/public/AdminLTE-3.0.1/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="/iis/public/AdminLTE-3.0.1/plugins/daterangepicker/daterangepicker.css">
    <!-- summernote -->
    <link rel="stylesheet" href="/iis/public/AdminLTE-3.0.1/plugins/summernote/summernote-bs4.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">

    {% block stylesheets %}{% endblock %}
</head>
<body class="hold-transition sidebar-mini layout-fixed">
<div class="wrapper">

    <!-- Navbar -->
    <nav class="main-header navbar navbar-expand navbar-white navbar-light">
        <!-- Left navbar links -->
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link" data-widget="pushmenu" href="#"><i class="fas fa-bars"></i></a>
            </li>
            <li class="nav-item d-none d-sm-inline-block">
                <a href="{{ url('homepage') }}" class="nav-link">Domovská stránka</a>
            </li>
        </ul>


        <!-- Right navbar links -->
        <ul class="navbar-nav ml-auto">
            <!-- Notifications Dropdown Menu -->
            <li class="nav-item dropdown">
                <a class="nav-link" data-toggle="dropdown" href="#">
                    <i class="far fa-user"></i>&nbsp;
                    Martin Buchta
                </a>
                <div class="dropdown-menu dropdown-menu-lg dropdown-menu-right">
                    <a href="{{ url('app_logout') }}" class="dropdown-item">
                        <i class="fas fa-sign-out-alt mr-2"></i> Odhlásit
                    </a>
                </div>
            </li>
        </ul>
    </nav>
    <!-- /.navbar -->

    <!-- Main Sidebar Container -->
    <aside class="main-sidebar sidebar-dark-primary elevation-4">
        <!-- Brand Logo -->
        <a href="{{ url("admin") }}" class="brand-link">
            <img src="https://medusa.fit.vutbr.cz/traffic/wp-content/logos/fit.png" alt="AdminLTE Logo" class="brand-image  elevation-3"
                 style="opacity: .8">
            <span class="brand-text font-weight-light" style="opacity: 0">VUT</span>
        </a>

        <!-- Sidebar -->
        <div class="sidebar">

            <!-- Sidebar Menu -->
            <nav class="mt-2">
                <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                    <!-- Add icons to the links using the .nav-icon class
                         with font-awesome or any other icon font library -->
                    <li class="nav-item">
                        <a href="{{ url('admin') }}" class="nav-link {% if app.request.pathInfo == path('admin') %}active{% endif %}">
                            <i class="nav-icon fas fa-tachometer-alt"></i>
                            <p>
                                Nástěnka
                            </p>
                        </a>
                    </li>
                    {% if is_granted('ROLE_ADMINISTRATOR') %}
                        <li class="nav-item">
                            <a href="{{ url('administrator_manage_users') }}" class="nav-link {% if app.request.pathInfo == path('administrator_manage_users') %}active{% endif %}">
                                <i class="nav-icon fas fa-user-alt"></i>
                                <p>
                                    Uživatelé
                                </p>
                            </a>
                        </li>
                    {% endif %}
                    {% if is_granted('ROLE_REDAKTOR') %}
                        <li class="nav-item">
                            <a href="{{ url('redaktor_hall_list') }}" class="nav-link {% if app.request.pathInfo == path('redaktor_hall_list') %}active{% endif %}">
                                <i class="nav-icon fas fa-building"></i>
                                <p>
                                    Sály
                                </p>
                            </a>
                        </li>
                    {% endif %}
                    {% if is_granted('ROLE_REDAKTOR') %}
                        <li class="nav-item">
                            <a href="{{ url('redaktor_play_list') }}" class="nav-link {% if app.request.pathInfo == path('redaktor_play_list') %}active{% endif %}">
                                <i class="nav-icon fas fa-building"></i>
                                <p>
                                    Inscenace
                                </p>
                            </a>
                        </li>
                    {% endif %}
                </ul>
            </nav>
            <!-- /.sidebar-menu -->
        </div>
        <!-- /.sidebar -->
    </aside>

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        {% set flashes = app.flashes %}
        {% if flashes | length > 0 %}
            <div style="padding: 15px">
                {% for label, messages in flashes %}
                    {% for message in messages %}
                        <div class="alert alert-{{ label }} alert-dismissible">
                            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                            {{ message }}
                        </div>
                    {% endfor %}
                {% endfor %}
            </div>
        {% endif %}
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </div>
    <!-- /.content-wrapper -->
    <footer class="main-footer">
        <strong>Copyright &copy; 2014-2019 <a href="http://adminlte.io">AdminLTE.io</a>.</strong>
        All rights reserved.
        <div class="float-right d-none d-sm-inline-block">
            <b>Version</b> 3.0.1
        </div>
    </footer>

    <!-- Control Sidebar -->
    <aside class="control-sidebar control-sidebar-dark">
        <!-- Control sidebar content goes here -->
    </aside>
    <!-- /.control-sidebar -->
</div>
<!-- ./wrapper -->

<!-- jQuery -->
<script src="/iis/public/AdminLTE-3.0.1/plugins/jquery/jquery.min.js"></script>
<!-- jQuery UI 1.11.4 -->
<script src="/iis/public/AdminLTE-3.0.1/plugins/jquery-ui/jquery-ui.min.js"></script>
<!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
<script>
    $.widget.bridge('uibutton', $.ui.button)
</script>
<!-- Bootstrap 4 -->
<script src="/iis/public/AdminLTE-3.0.1/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
<!-- ChartJS -->
<script src="/iis/public/AdminLTE-3.0.1/plugins/chart.js/Chart.min.js"></script>
<!-- Sparkline -->
<script src="/iis/public/AdminLTE-3.0.1/plugins/sparklines/sparkline.js"></script>
<!-- JQVMap -->
<script src="/iis/public/AdminLTE-3.0.1/plugins/jqvmap/jquery.vmap.min.js"></script>
<script src="/iis/public/AdminLTE-3.0.1/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
<!-- jQuery Knob Chart -->
<script src="/iis/public/AdminLTE-3.0.1/plugins/jquery-knob/jquery.knob.min.js"></script>
<!-- daterangepicker -->
<script src="/iis/public/AdminLTE-3.0.1/plugins/moment/moment.min.js"></script>
<script src="/iis/public/AdminLTE-3.0.1/plugins/daterangepicker/daterangepicker.js"></script>
<!-- Tempusdominus Bootstrap 4 -->
<script src="/iis/public/AdminLTE-3.0.1/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
<!-- Summernote -->
<script src="/iis/public/AdminLTE-3.0.1/plugins/summernote/summernote-bs4.min.js"></script>
<!-- overlayScrollbars -->
<script src="/iis/public/AdminLTE-3.0.1/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
<!-- AdminLTE App -->
<script src="/iis/public/AdminLTE-3.0.1/dist/js/adminlte.js"></script>
<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
<script src="/iis/public/AdminLTE-3.0.1/dist/js/pages/dashboard.js"></script>
<!-- AdminLTE for demo purposes -->
<script src="/iis/public/AdminLTE-3.0.1/dist/js/demo.js"></script>
<script src="https://cdn.ckeditor.com/ckeditor5/15.0.0/classic/ckeditor.js"></script>

<script>
    ClassicEditor.defaultConfig = {
        toolbar: [ 'heading', '|', 'bold', 'italic', 'link', 'bulletedList', 'numberedList', 'blockQuote'
        ],

        // This value must be kept in sync with the language defined in webpack.config.js.
        language: 'cs'
    };
    ClassicEditor
        .create( document.querySelector( '.ckeditor' ) )
        .then( editor => {
            console.log( editor );
        } )
        .catch( error => {
            console.error( error );
        } );
</script>

</body>
</html>
